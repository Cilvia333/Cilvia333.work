{
  "presets": [
    [
      "@babel/preset-env",
      { "targets": { "browsers": [">0.25%", "not dead"] }}
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-proposal-optional-chaining",
    "@babel/plugin-proposal-class-properties",
    [
      "macros",
      {
        "twin":{
          "config": "tailwind.config.js",
          "preset": "styled-components",
          "autoCssProp": true,
          "debugProp": true,
          "debugPlugins": false,
          "debug": false
        }
      }
    ]
  ]
}